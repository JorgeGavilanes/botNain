{
    "version" : 2,
    "builds" : [
        {
            "src" : "./index.js",
            "use" : "@vercel/node"
        }
    ],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "/"
        }
    ],
    "functions" : {
        "**/*.js":{
            "includeFiles":".wwebjs_auth/session"
        }
    }
}